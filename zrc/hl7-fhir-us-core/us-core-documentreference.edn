    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.document-reference-status/value-set,
     :strength :required}},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:MS}}},
   :context
   {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
    :type zen/map,
    :keys
    {:encounter
     {:type zen/vector,
      :every
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :fhir/flags #{:MS},
       :zen.fhir/reference
       {:refers #{hl7-fhir-us-core.us-core-encounter/schema}}},
      :maxItems 1},
     :period
     {:confirms #{hl7-fhir-r4-core.Period/schema},
      :fhir/flags #{:MS}}},
    :fhir/flags #{:MS}},
   :subject
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:MS},
    :zen.fhir/reference
    {:refers #{hl7-fhir-us-core.us-core-patient/schema}}}},
  :zen.fhir/type "DocumentReference"}}